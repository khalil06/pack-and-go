<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane
        fx:id="pane"
        xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.company.gui.MenuController"
            prefHeight="400.0" prefWidth="600.0"
stylesheets="@bootstrap.css">
    <center>
        <FlowPane orientation="VERTICAL" alignment="CENTER" columnHalignment="CENTER" rowValignment="CENTER"
                  prefHeight="50.00" style="-fx-border-color: red; -fx-border-width: 3; -fx-border-style: dashed">
            <Label fx:id="text" text="Want to check your personality?"/>
            <Button fx:id="button" text="start" alignment="CENTER" onAction="#onButtonClicked" styleClass="btn-success"/>
        </FlowPane>
    </center>
</BorderPane>
